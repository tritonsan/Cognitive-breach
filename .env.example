# ============================================================
# COGNITIVE BREACH - Environment Configuration Template
# ============================================================
# Copy this file to .env and fill in your actual values
# ============================================================

# Google Gemini API Key (REQUIRED)
# Get yours at: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Gemini Model Selection (OPTIONAL)
# Options: gemini-3-flash-preview, gemini-3-pro, gemini-3-ultra-preview
GEMINI_MODEL=gemini-3-flash-preview

# Debug Mode (OPTIONAL)
# Set to true to enable verbose logging and internal monologue display
DEBUG=false
